    "hbs": "^4.0.1",
    "httpreq": "^0.4.24",
    "log4js": "^2.5.3",
    "lusca": "^1.6.0",
    "memory-cache": "^0.2.0",
    "mockjs": "^1.0.1-beta3",
    "modex": "^2.0.1",
    "moment": "^2.19.3",
    "mongoose": "5.0.16",
    "mysql": "^2.15.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
